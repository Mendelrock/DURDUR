package commandes;

import java.util.GregorianCalendar;

public class Payée extends ACommande{

	public Payée(int numero) {
		super(numero);
	}

	@Override
	public void payer() {
		throw new CommandeDéjàPayéeEx();
	}

	@Override
	public void livrer() {
		this.dateLiv = new GregorianCalendar();
	}

	@Override
	public void rembourser() {
		// TODO Auto-generated method stub
		
	}

	@Override
	String getEtat() {
		return  "Payée";
	}

}

package commandes;

public class Commande extends EtatCommande {
	
	public String getEtat() {
		if (payee == false)
			return "Créée";
		else if (dateLiv == null)
			return  "Payée";
		else return "Livrée";
	}
	public int getNuméro() {return numero;}
	public Commande(int numero) {
		this.numero = numero;
		this.payee = false;
	}
}
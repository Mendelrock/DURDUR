package commandes;

public class NonPayée extends ACommande{

	public NonPayée(int numero) {
		super(numero);
		// TODO Auto-generated constructor stub
	}

	@Override
	public void payer() {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void livrer() {
		throw new CommandeNonLivrableEx();
	}

	@Override
	public void rembourser() {
		throw new CommandeNonRemboursableEx ();
	}

	@Override
	String getEtat() {
		return "Créée";
	}

}

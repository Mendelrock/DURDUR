package commandes;

public class Livrée extends ACommande {

	public Livrée(int numero) {
		super(numero);
		// TODO Auto-generated constructor stub
	}

	@Override
	public void payer() {
		throw new CommandeDéjàPayéeEx();
	}

	@Override
	public void livrer() {
		throw new CommandeNonLivrableEx();
	}

	@Override
	public void rembourser() {
		throw new CommandeNonRemboursableEx ();

	}

	@Override
	String getEtat() {
		return "Livrée";
	}

}

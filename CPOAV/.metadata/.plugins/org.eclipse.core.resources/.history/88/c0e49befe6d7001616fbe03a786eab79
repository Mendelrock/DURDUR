package hopital;

public class Evalué implements IEtatPatient {

	@Override
	public String getDoc(Patient pat) {
		throw new PatientPasAffecté();
	}

	@Override
	public void évaluer(Patient pat) {
		throw new PatientPasEnAttente();
	}

	@Override
	public void affecter(String doc, Patient pat) {
		pat.doc=doc
		
	}

	@Override
	public void traiter(Patient pat) {
		// TODO Auto-generated method stub
		
	}

}
